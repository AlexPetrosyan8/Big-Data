        "#2. Suggest a methodology and make an estimate for the whole data given that transactions.csv is a random sample of 3%\n",
        "#from the entire population (3 points). Estimate\n",
        " #   a. total revenue\n",
        " #  b. number of customers\n",
        " # c. number of transactions\n",
        "#d. average expenses per year per customer"
      ],
      "metadata": {
        "id": "UnzxwpaPx1I-"
      },
      "execution_count": 60,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "from pyspark.sql.functions import sum, countDistinct, lit, count, to_date, year, col"
      ],
      "metadata": {
        "id": "prlxpZ8Cxm27"
      },
      "execution_count": 61,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Define the scaling factor\n",
        "EXTRAPOLATION_FACTOR = 1 / 0.03"
      ],
      "metadata": {
        "id": "RXLl8GlMxVXg"
      },
      "execution_count": 62,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "#Calculate Sample Metrics\n",
        "sample_metrics = transactions_df.agg(\n",
        "    sum(\"price\").alias(\"sample_total_revenue\"),\n",
        "    countDistinct(\"customer_id\").alias(\"sample_total_customers\"),\n",
        "    count(lit(1)).alias(\"sample_total_transactions\")\n",
        ").collect()[0]"
      ],
      "metadata": {
        "id": "lieewVihyPO3"
      },
      "execution_count": 64,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Extract Sample Values\n",
        "S_Revenue = sample_metrics[\"sample_total_revenue\"]\n",
        "S_Customers = sample_metrics[\"sample_total_customers\"]\n",
        "S_Transactions = sample_metrics[\"sample_total_transactions\"]"
      ],
      "metadata": {
        "id": "QF55rGuiyX0s"
      },
      "execution_count": 65,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "#Determine the number of years in the sample for calculation average expenses per year per customer"
      ],
      "metadata": {
        "id": "1LQQV1qpyd-w"
      },
      "execution_count": 66,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "dates_df = transactions_df.withColumn(\"t_dat\", to_date(col(\"t_dat\"), \"yyyy-MM-dd\"))"
      ],
      "metadata": {
        "id": "6YuDG2jTyw9p"
      },
      "execution_count": 67,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "min_year_val = dates_df.select(year(\"t_dat\")).agg({\"year(t_dat)\": \"min\"}).collect()[0][0]\n",
        "max_year_val = dates_df.select(year(\"t_dat\")).agg({\"year(t_dat)\": \"max\"}).collect()[0][0]"
      ],
      "metadata": {
